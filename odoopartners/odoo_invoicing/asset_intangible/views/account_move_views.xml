<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="account_move_view_form_inherit_asset_intangible" model="ir.ui.view">
        <field name="name">account.move.view.form.inherit.asset.intangible</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='invoice_line_ids']//tree" position="inside">
                <field name="asset_intangible_id" column_invisible="1"/>
            </xpath>
            <xpath expr="//field[@name='line_ids']//tree" position="inside">
                <field name="asset_intangible_id" optional="hide"/>
            </xpath>
        </field>
    </record>

</odoo>
