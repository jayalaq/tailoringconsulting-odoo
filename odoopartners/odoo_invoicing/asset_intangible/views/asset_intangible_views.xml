<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="asset_intangible_view_form" model="ir.ui.view">
        <field name="name">asset.intangible.view.form</field>
        <field name="model">asset.intangible</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="bool_asset_intagible"/>
                        <field name="operation_date"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="asset_intangible_view_tree" model="ir.ui.view">
        <field name="name">asset.intangible.view.tree</field>
        <field name="model">asset.intangible</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="bool_asset_intagible"/>
                <field name="operation_date"/>
            </tree>
        </field>
    </record>

    <record id="asset_intangible_view_search" model="ir.ui.view">
        <field name="name">asset.intangible.view.search</field>
        <field name="model">asset.intangible</field>
        <field name="arch" type="xml">
            <search>
                <field name="name" string="Asset / Intangible"/>
                <field name="bool_asset_intagible" string="is the asset / intangible active"/>
                <filter string="Asset/Intangible" name="name"/>
                <filter string="is the asset/intangible active" name="bool_asset_intagible"/>
            </search>
        </field>
    </record>

</odoo>
