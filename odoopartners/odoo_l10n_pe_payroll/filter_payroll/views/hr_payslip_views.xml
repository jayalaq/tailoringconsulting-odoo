<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="hr_payslip_view_form_inherit_filter_payroll" model="ir.ui.view">
        <field name="name">hr.payslip.view.form.inherit.filter_payroll</field>
        <field name="model">hr.payslip</field>
        <field name="inherit_id" ref="hr_payroll.view_hr_payslip_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page/field[@name='worked_days_line_ids']/tree/field[@name='sequence']" position="after">
                <field name="is_social_benefits_license" column_invisible="true" invisible="1"/>
                <field name="is_benefits_license_absence" column_invisible="true" invisible="1"/>
                <field name="is_calc_own_rule" column_invisible="true" invisible="1"/>
                <field name="unpaid" invisible="0"/>
            </xpath>
        </field>
    </record>
</odoo>