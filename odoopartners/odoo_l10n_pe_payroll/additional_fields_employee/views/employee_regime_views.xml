<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="employee_regime_view_form" model="ir.ui.view">
        <field name="name">employee.regime.view.form</field>
        <field name="model">employee.regime</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="[33] Régimen Laboral">
                <sheet>
                    <group>
                        <field name="code"/>
                        <field name="regime_description"/>
                        <field name="name"/>
                        <field name="private_sector"/>
                        <field name="public_sector"/>
                        <field name="other_entities"/>
                        <field name="is_mype"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="employee_regime_view_tree" model="ir.ui.view">
        <field name="name">employee.regime.view.tree</field>
        <field name="model">employee.regime</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="[33] Régimen Laboral">
                <field name="code"/>
                <field name="regime_description"/>
                <field name="name"/>
                <field name="private_sector"/>
                <field name="public_sector"/>
                <field name="other_entities"/>
                <field name="is_mype"/>
            </tree>
        </field>
    </record>

</odoo>
