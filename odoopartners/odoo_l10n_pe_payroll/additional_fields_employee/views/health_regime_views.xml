<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="health_regime_view_tree" model="ir.ui.view">
        <field name="name">health.regime.view.tree</field>
        <field name="model">health.regime</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree>
                <field name="code"/>
                <field name="health_description"/>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="health_regime_view_form" model="ir.ui.view">
        <field name="name">health.regime.view.form</field>
        <field name="model">health.regime</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="code"/>
                        <field name="health_description"/>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
