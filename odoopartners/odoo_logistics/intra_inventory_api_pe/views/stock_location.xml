<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="stock_location_view_form_inherit_intra_inventory_api_pe" model="ir.ui.view">
        <field name="name">stock.location.view.form.inherit.intra_inventory_api_pe</field>
        <field name="model">stock.location</field>
        <field name="inherit_id" ref="stock.view_location_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='replenish_location']" position="after">
                <field name="intralot"/>
            </xpath>
        </field>
    </record>
</odoo>