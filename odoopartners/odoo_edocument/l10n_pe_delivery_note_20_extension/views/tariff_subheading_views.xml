<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="tariff_subheading_view_tree" model="ir.ui.view">
        <field name="name">tariff.subheading.view.tree</field>
        <field name="model">tariff.subheading</field>
        <field name="arch" type="xml">
            <tree>
                <field name="code"/>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="tariff_subheading_view_form" model="ir.ui.view">
        <field name="name">tariff.subheading.view.form</field>
        <field name="model">tariff.subheading</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="code"/>
                        </group>
                        <group>
                            <field name="name"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="tariff_subheading_action" model="ir.actions.act_window">
        <field name="name">Sub-partida arancelaria SUNAT</field>
        <field name="res_model">tariff.subheading</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_tariff_subheading" name="Sub-partida arancelaria SUNAT" action="tariff_subheading_action" parent="menu_electronic_guide" sequence="3"/>

</odoo>
