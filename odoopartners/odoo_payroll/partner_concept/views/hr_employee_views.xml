<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="hr_employee_view_form_inherit_partner_concept" model="ir.ui.view">
        <field name="name">hr.employee.view.form.inherit.partner_concept</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Conceptos Salariales">
                    <field name="partner_concept_ids"/>
                </page>
            </xpath>
        </field>
    </record>

</odoo>
