<?xml version='1.0' encoding='utf-8'?>
<odoo>
    
    <record id="code_aduana_view_form" model="ir.ui.view">
        <field name="name">code.aduana.view.form</field>
        <field name="model">code.aduana</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="code"/>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="code_aduana_view_tree" model="ir.ui.view">
        <field name="name">code.aduana.view.tree</field>
        <field name="model">code.aduana</field>
        <field name="arch" type="xml">
            <tree>
                <field name="code"/>
                <field name="name"/>
            </tree>
        </field>
    </record>

</odoo>