<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.customize_later_delivery_for_pos</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="point_of_sale.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//setting[@id='barcode_scanner']" position="before">
                <setting id="flexible_ship_later_div">
                    <field name="pos_flexible_ship_later"/>
                </setting>
            </xpath>
        </field>
    </record>

</odoo>
