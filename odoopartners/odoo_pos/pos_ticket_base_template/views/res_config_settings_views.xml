<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="res_config_settings_view_form_inherit_pos_ticket_base_template" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.pos_ticket_base_template</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="point_of_sale.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//setting[@id='auto_printing']" position="after">
                <setting id="automatic_print_electronic_invoice">
                    <field name="pos_automatic_print_electronic_invoice"/>
                </setting>
                <setting id="automatic_download_electronic_invoice">
                    <field name="pos_automatic_download_electronic_invoice"/>
                </setting>
            </xpath>
        </field>
    </record>

</odoo>
